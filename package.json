{
  "name": "@stackoverflow/stacks",
  "description": "Stack Overflowâ€™s CSS and Design Pattern Library. Stacks is an atomic CSS library with classes and components for rapidly building Stack Overflow.",
  "repository": {
    "type": "git",
    "url": "https://github.com/StackExchange/Stacks.git"
  },
  "version": "1.7.1",
  "files": [
    "dist",
    "lib"
  ],
  "main": "./dist/js/stacks.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/css/stacks.css",
  "less": "./lib/css/stacks.less",
  "unpkg": "dist/css/stacks.min.css",
  "scripts": {
    "start": "concurrently -n w: npm:start:*",
    "build": "concurrently -n w: npm:build:*",
    "build:bundle": "webpack --mode=production",
    "build:docs": "webpack --mode=production --config ./docs/webpack.config.js && cd ./docs && eleventy",
    "start:webpack": "webpack --watch --config ./docs/webpack.config.js",
    "start:eleventy": "cd ./docs && eleventy --serve",
    "test": "web-test-runner",
    "test:a11y": "web-test-runner --group=a11y",
    "test:unit": "web-test-runner --group=unit",
    "test:unit:watch": "web-test-runner --group=unit --watch",
    "test:visual": "web-test-runner --group=visual",
    "test:visual:update": "web-test-runner --group=visual --update-visual-baseline",
    "prepublishOnly": "npm run build",
    "lint": "concurrently -n w: npm:lint:*",
    "lint:ts": "eslint ./lib/ts",
    "lint:css": "stylelint ./lib/css",
    "lint:format": "prettier --check ./lib"
  },
  "license": "MIT",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.1",
    "@popperjs/core": "^2.11.6"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@highlightjs/cdn-assets": "^11.7.0",
    "@open-wc/testing": "^3.1.7",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@stackoverflow/stacks-editor": "^0.8.4",
    "@stackoverflow/stacks-icons": "^5.0.2",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/dev-server-rollup": "0.3.21",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@web/test-runner-visual-regression": "^0.7.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.14",
    "docsearch.js": "^2.6.3",
    "eleventy-plugin-highlightjs": "^1.1.0",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "jquery": "^3.6.3",
    "less-loader": "^11.1.0",
    "list.js": "^2.3.1",
    "markdown-it": "^13.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.2",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie > 0",
    "not op_mini all",
    "not baidu > 0",
    "not and_ff > 0",
    "not and_qq > 0",
    "not kaios > 0",
    "not op_mob > 0",
    "not ie_mob > 0",
    "not and_uc > 0",
    "not Samsung > 0",
    "not Android > 0"
  ],
  "overrides": {
    "aria-query": "~5.0.0"
  }
}
