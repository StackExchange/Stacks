{
  "name": "@stackoverflow/stacks",
  "description": "Stack Overflowâ€™s CSS and Design Pattern Library. Stacks is an atomic CSS library with classes and components for rapidly building Stack Overflow.",
  "repository": {
    "type": "git",
    "url": "https://github.com/StackExchange/Stacks.git"
  },
  "version": "0.74.0",
  "files": [
    "dist",
    "lib/css"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/types/index.d.ts",
  "less": "lib/css/stacks.less",
  "style": "dist/css/stacks.css",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "unpkg": "dist/css/stacks.min.css",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:grunt": "grunt build",
    "build:ts-cjs": "tsc -p tsconfig.json && echo {\\\"type\\\": \\\"commonjs\\\"} > dist/cjs/package.json",
    "build:ts-mjs": "tsc -p tsconfig-mjs.json && echo {\\\"type\\\": \\\"module\\\"} > dist/mjs/package.json",
    "build:ts-cdn": "webpack --config=webpack.release.ts",
    "docs:build": "cd docs && eleventy",
    "docs:serve": "cd docs && eleventy --serve",
    "deploy": "npm run build & npm run docs:build",
    "start": "npm run build & npm run docs:serve",
    "test": "backstop test",
    "approve": "backstop approve",
    "webpack": "webpack --config=webpack.debug.ts",
    "prepare": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "stimulus": "^2.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@highlightjs/cdn-assets": "^11.4.0",
    "@stackoverflow/stacks-editor": "^0.4.2",
    "@stackoverflow/stacks-icons": "^2.27.0",
    "backstopjs": "^6.0.4",
    "concurrently": "^7.0.0",
    "docsearch.js": "^2.6.3",
    "eleventy-plugin-highlightjs": "^0.3.0",
    "eleventy-plugin-markdown-shortcode": "^1.1.0",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "esbuild-loader": "^2.18.0",
    "grunt": "^1.4.1",
    "grunt-cli": "^1.4.3",
    "grunt-concurrent": "^3.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-less": "^3.0.0",
    "grunt-contrib-uglify": "^5.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-shell": "^3.0.1",
    "grunt-ts": "^6.0.0-beta.22",
    "jquery": "^3.6.0",
    "list.js": "^2.3.1",
    "postcss-less": "^6.0.0",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-less": "^1.0.3",
    "terser-webpack-plugin": "^5.3.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.2",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie > 0",
    "not op_mini all",
    "not baidu > 0",
    "not and_ff > 0",
    "not and_qq > 0",
    "not kaios > 0",
    "not op_mob > 0",
    "not ie_mob > 0",
    "not and_uc > 0",
    "not Samsung > 0",
    "not Android > 0"
  ]
}
